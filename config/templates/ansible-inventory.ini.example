[all:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_python_interpreter=/usr/bin/python3

[master]
k8s-master ansible_host=MASTER_PUBLIC_IP

[workers]
k8s-worker-1 ansible_host=WORKER_1_PUBLIC_IP
k8s-worker-2 ansible_host=WORKER_2_PUBLIC_IP

[kubernetes:children]
master
workers
